{
    "$defs": {
        "ReactivePowerSetting": {
            "description": "Reactive power setting, almost always VARAVAL_WATTS.",
            "enum": [
                "VARAVAL_WATTS",
                "VARMAX_VARS",
                "VARMAX_WATTS"
            ],
            "title": "ReactivePowerSetting",
            "type": "string"
        }
    },
    "description": "OpenDSS setting for volt-var control.",
    "properties": {
        "deltaq_factor": {
            "default": -1.0,
            "title": "Deltaq Factor",
            "type": "number"
        },
        "varchangetolerance": {
            "default": 0.025,
            "title": "Varchangetolerance",
            "type": "number"
        },
        "voltagechangetolerance": {
            "default": 0.0001,
            "title": "Voltagechangetolerance",
            "type": "number"
        },
        "vv_refreactivepower": {
            "allOf": [
                {
                    "$ref": "#/$defs/ReactivePowerSetting"
                }
            ],
            "default": "VARAVAL_WATTS"
        },
        "voltage": {
            "items": {
                "type": "number"
            },
            "title": "Voltage",
            "type": "array"
        },
        "reactive_response": {
            "items": {
                "type": "number"
            },
            "title": "Reactive Response",
            "type": "array"
        }
    },
    "required": [
        "voltage",
        "reactive_response"
    ],
    "title": "VVControl",
    "type": "object"
}